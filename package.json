{
  "name": "@lifeomic/chromicons",
  "version": "0.0.4",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/chromicons.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": "git@github.com:lifeomic/chromicons.git",
  "author": "LifeOmic <oss@lifeomic.com>",
  "license": "MIT",
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "clean": "rm -rf dist/ && rm src/index.tsx && rm -rf src/lined/",
    "generate:build-output": "yarn generate:react-components && yarn generate:index-file",
    "generate:index-file": "echo \"export * from './lined'\" > src/index.tsx",
    "generate:react-components": "svgr --template svg-template.js lined/ --out-dir src/lined --typescript",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.8.0",
    "@svgr/cli": "^5.5.0",
    "@types/react-dom": "^16.9.9",
    "@types/react": "^16.14.0",
    "husky": "^4.3.0",
    "react-dom": "^17.0.1",
    "react": "^17.0.1",
    "size-limit": "^4.8.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn size"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/chromicons.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chromicons.esm.js",
      "limit": "15 KB"
    }
  ]
}
